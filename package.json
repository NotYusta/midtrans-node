{
  "name": "hanif-midtrans-node",
  "version": "1.0.7",
  "description": "Midtrans API Wrapper for NodeJS with typescript.",
  "main": "./compiles/index.js",
  "repository": "https://github.com/hansputera/midtrans-node",
  "author": "Hanif",
  "license": "MIT",
  "devDependencies": {
    "@types/luxon": "^2.0.7",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "mocha": "^9.1.3",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "luxon": "^2.1.1"
  },
  "scripts": {
    "build": "rimraf compiles && tsc --build tsconfig.json",
    "lint": "eslint \"./src/**/*.ts\" --fix",
    "test": "mocha",
    "prepare": "husky install",
    "format": "prettier --write \"./src/**/*.+(ts|.d.ts)\"",
    "docs": "typedoc"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
